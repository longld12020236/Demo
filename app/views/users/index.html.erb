<% provide(:title, 'All users') %>
<h1 style="color: white">All users</h1>
<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user , style: "color: white" %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>